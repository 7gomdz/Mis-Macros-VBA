Attribute VB_Name = "M�dulo1"
Sub CompletarDigitosSilencioso()
    Dim fila As Long
    Dim ultimaFila As Long
    Dim partes() As String
    
    ' Optimizaci�n: Silencia el parpadeo de pantalla
    Application.ScreenUpdating = False
    
    ' 1. Limpiar la columna B (desde B2 hasta el final)
    Range("B2:B" & Rows.Count).ClearContents
    
    ' 2. Encontrar la �ltima fila con datos en la columna A
    ultimaFila = Cells(Rows.Count, 1).End(xlUp).Row
    
    ' Validar si hay datos (Si solo est� el encabezado, termina)
    If ultimaFila < 2 Then Exit Sub
    
    ' 3. Procesar datos
    For fila = 2 To ultimaFila
        If InStr(Cells(fila, 1).Value, "-") > 0 Then
            partes = Split(Cells(fila, 1).Value, "-")
            Cells(fila, 2).Value = partes(0) & "-" & Format(partes(1), "0000000000")
        End If
    Next fila
    
    ' Volver a activar la actualizaci�n de pantalla
    Application.ScreenUpdating = True
End Sub
